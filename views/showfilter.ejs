<%- include("partials/header"); -%>
    <%query.forEach((element)=>{%>
        <h1>
            <%=element.title%>
          </h1>
          <h2>
            <%=element.postoffer%>
          </h2>
          <h3>
            <%=element.difficulty%>
          </h3>
          <p>
            <%element.topictags.forEach(function(topictag){%>
              <h3>
                <%=topictag%>
              </h3>
              <%})%>
          </p>
    <%})%>
<%- include("partials/footer"); -%>